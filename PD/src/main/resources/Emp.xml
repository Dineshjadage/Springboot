<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean id="a1" class="com.example.demo.Address">
<property name="city" value="LA"></property>
<property name="state" value="california"></property>
<property name="country" value="USA"></property>
</bean>
<bean id="e1" class="com.example.demo.Employee">
<property name="id" value="1"></property>
<property name="name" value="Rose"></property>
 <property name="address" ref="a1"></property>
 
</bean>
</beans>
